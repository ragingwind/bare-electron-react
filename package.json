{
  "name": "bare-electron-react",
  "productName": "Electron with React",
  "version": "0.0.0",
  "description": "Electron with React",
  "license": "MIT",
  "main": "index.js",
  "repository": "ragingwind/bare-electron-react",
  "author": {
    "name": "ragingwind",
    "email": "ragingwind@gmail.com",
    "url": "ragingwind.me"
  },
  "engines": {
    "node": ">=0.12.0"
  },
  "build": {
    "version": "0.29.1",
    "target": "./build"
  },
  "scripts": {
    "start": "electron .",
    "clean": "rm -rf $npm_package_build_target",
    "build": "if [ -z $PLATFORM ]; then PLATFORM=all; fi; if [ -z $ARCH ]; then ARCH=all; fi; electron-packager . $npm_package_productName --out=$npm_package_build_target --platform=$PLATFORM --arch=$ARCH --prune --asar --version=$npm_package_build_version",
    "build:darwin": "PLATFORM=darwin ARCH=x64 npm run packager",
    "build:win32": "PLATFORM=win32 ARCH=all npm run packager",
    "build:linux": "PLATFORM=linux ARCH=all npm run packager"
  },
  "files": [
    "index.js",
    "index.html",
    "index.css"
  ],
  "keywords": [
    "electron-app"
  ],
  "devDependencies": {
    "electron-packager": "5.0.0",
    "electron-prebuilt": "^0.29.1"
  },
  "dependencies": {
    "babel": "^5.8.5",
    "electron-debug": "^0.1.1",
    "react": "^0.13.3"
  }
}
