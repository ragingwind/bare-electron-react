{
  "name": "bare-electron-react",
  "productName": "Electron with React",
  "version": "0.0.0",
  "description": "Electron with React",
  "license": "MIT",
  "main": "index.js",
  "repository": "ragingwind/bare-electron-react",
  "author": {
    "name": "ragingwind",
    "email": "ragingwind@gmail.com",
    "url": "ragingwind.me"
  },
  "engines": {
    "node": ">=0.12.0"
  },
  "config": {},
  "scripts": {
    "copy": "cpy './**/*.*' ../debug --parents --cwd=app --ignore='./**/*.jsx,./**/*.es6'",
    "babel": "babel app -d debug",
    "app": "mkdir -p debug; npm run babel; npm run copy; electron ./debug",
    "clean": "rm -rf $npm_package_configs_build debug",
    "build": "if [ -z $PLATFORM ]; then PLATFORM=all; fi; if [ -z $ARCH ]; then ARCH=all; fi; electron-packager ./debug $npm_package_productName --out=./build --platform=$PLATFORM --arch=$ARCH --prune --asar --version=0.29.1"
  },
  "files": [
    "index.js",
    "index.html",
    "index.css"
  ],
  "keywords": [
    "electron-app"
  ],
  "devDependencies": {
    "cpy": "git://github.com/ragingwind/cpy",
    "electron-packager": "5.0.0",
    "electron-prebuilt": "^0.29.1"
  },
  "dependencies": {
    "babel": "^5.8.5",
    "electron-debug": "^0.1.1",
    "react": "^0.13.3"
  }
}
